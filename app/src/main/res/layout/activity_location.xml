<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LocationActivity">

    <ImageButton
        android:id="@+id/img_btn_back2"
        android:layout_width="198dp"
        android:layout_height="73dp"
        android:layout_marginTop="-10dp"
        android:backgroundTint="#E8AA4F"
        android:onClick="Back"
        app:layout_constraintEnd_toStartOf="@+id/tooldbar_wasabi2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_arrow_back" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tooldbar_wasabi2"
        android:layout_width="310dp"
        android:layout_height="57dp"
        android:background="?attr/colorPrimary"
        android:backgroundTint="#E8AA4F"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/img_btn_home2"
        android:layout_width="125dp"
        android:layout_height="72dp"
        android:background="#00FFFFFF"
        android:onClick="Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/img_btn_mais2"
        app:layout_constraintStart_toEndOf="@+id/img_btn_location2"
        app:srcCompat="@drawable/ic_home"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/img_btn_location2"
        android:layout_width="125dp"
        android:layout_height="72dp"
        android:layout_marginEnd="250dp"
        android:background="#fff"
        android:backgroundTint="#fff"
        android:onClick="Mapas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_location_orange"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/img_btn_mais2"
        android:layout_width="125dp"
        android:layout_height="72dp"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="25dp"
        android:background="#00FFFFFF"
        android:onClick="Mais"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_btn_home2"
        app:layout_constraintTop_toBottomOf="@+id/btn_mapa"
        app:srcCompat="@drawable/ic_add"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="248dp"
        android:layout_height="327dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="80dp"
        app:layout_constraintBottom_toTopOf="@+id/img_btn_home2"
        app:layout_constraintEnd_toStartOf="@+id/img_btn_mais2"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/tomago" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="71dp"
        android:text="@string/aprensetacao_gogo"
        android:textColor="#C0535353"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="50dp"
        android:text="@string/mapa"
        android:textColor="#E8AA4F"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/btn_mapa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="200dp"
        android:backgroundTint="#E8AA4F"
        android:text="MAPA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tooldbar_wasabi2" />

</androidx.constraintlayout.widget.ConstraintLayout>