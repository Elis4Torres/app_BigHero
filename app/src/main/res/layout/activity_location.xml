<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LocationActivity">

    <ImageButton
        android:id="@+id/img_btn_back2"
        android:layout_width="199dp"
        android:layout_height="79dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="225dp"
        android:backgroundTint="#FFF27F"
        android:onClick="Back"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toStartOf="@+id/toolbar3"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_arrow_back" />

    <ImageButton
        android:id="@+id/img_btn_home2"
        android:layout_width="125dp"
        android:layout_height="72dp"
        android:layout_marginTop="500dp"
        android:background="#00FFFFFF"
        android:onClick="Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/img_btn_mais2"
        app:layout_constraintStart_toEndOf="@+id/img_btn_location2"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3"
        app:layout_constraintVertical_bias="0.51"
        app:srcCompat="@drawable/ic_home"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/img_btn_location2"
        android:layout_width="125dp"
        android:layout_height="72dp"
        android:layout_marginTop="500dp"
        android:layout_marginEnd="250dp"
        android:background="#fff"
        android:backgroundTint="#fff"
        android:onClick="Mapas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3"
        app:srcCompat="@drawable/ic_location_amarelo"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/img_btn_mais2"
        android:layout_width="125dp"
        android:layout_height="72dp"
        android:layout_marginTop="530dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="25dp"
        android:background="#00FFFFFF"
        android:onClick="Mais"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_btn_home2"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3"
        app:srcCompat="@drawable/ic_add"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="326dp"
        android:layout_height="60dp"
        android:layout_marginEnd="1dp"
        android:background="?attr/colorPrimary"
        android:backgroundTint="#FFF27F"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="2dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="248dp"
        android:layout_height="327dp"
        android:layout_marginTop="258dp"
        android:layout_marginEnd="80dp"
        app:layout_constraintBottom_toTopOf="@+id/img_btn_home2"
        app:layout_constraintEnd_toStartOf="@+id/img_btn_mais2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3"
        app:srcCompat="@drawable/corrida" />

</androidx.constraintlayout.widget.ConstraintLayout>